{
  "functionName": "${project}-${stage}-${name}",
  "apiResponseTemplate": {
    "204 No Content:.*": {
      "statusCode": "204"
    },
    "400 Bad Request:.*": {
      "statusCode": "400"
    },
    "401 Unauthorized:.*": {
      "statusCode": "401"
    },
    "403 Unauthorized request:.*": {
      "statusCode": "403"
    },
    "404 Not Found:.*": {
      "statusCode": "404"  
    },
    "500 Internal Server Error:.*": {
      "statusCode": "500"
    },
    "default": {
      "statusCode": "200",
      "responseParameters": {},
      "responseModels": {
        "application/json;charset=UTF-8": "Empty"
      },
      "responseTemplates": {
        "application/json;charset=UTF-8": ""
      }
    }
  }
}
